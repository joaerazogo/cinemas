<div class="center">
  <h1><strong><i>LISTA DE CLIENTES EXISTENTES</i></strong></h1>
</div>
<form class="" action="" method="post">
  <div class="center">
    <h1>Bienvenido, por favor seleccione el correo del cliente que desea buscar</h1>
  </div>

    {{#power-select
      options=model.clients
      searchField="email"
      selected=selected
      onchange=(action "onChangeEmail") as |client|}}
        {{client.email}}
    {{/power-select}}

  <div class="center">
    <div class="row">
      {{#link-to 'index'}}
        <button class="btn waves-effect waves-light" type="submit" name="action" >Cancelar
          <i class="material-icons right">arrow_back</i>
        </button>
      {{/link-to}}
      {{#link-to 'client.readclient.readclients' selectedEmail}}
        <button class="btn waves-effect waves-light" type="submit" name="action">Leer
          <i class="material-icons right">arrow_forward</i>
        </button>
      {{/link-to}}
    </div>
  </div>
</form>
{{outlet}}
